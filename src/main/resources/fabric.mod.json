            "me.bionicbeanie.mods.savecoords.SaveCoordinatesClient"
        ],
        "modmenu": [
            "me.bionicbeanie.mods.savecoords.modmenu.SaveCoordinatesModMenu"
        ]
    },
    "mixins": [],
    "depends": {
        "fabricloader": ">=0.11.3",
